{
  "name": "virtua",
  "version": "0.44.0",
  "description": "A zero-config, fast and small (~3kB) virtual list (and grid) component for React, Vue, Solid and Svelte.",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "default": "./lib/index.js"
    },
    "./vue": {
      "types": "./lib/vue/index.d.ts",
      "require": "./lib/vue/index.cjs",
      "default": "./lib/vue/index.js"
    },
    "./solid": {
      "types": "./lib/solid/index.d.ts",
      "solid": "./lib/solid/index.jsx",
      "require": "./lib/solid/index.cjs",
      "default": "./lib/solid/index.js"
    },
    "./svelte": {
      "types": "./lib/svelte/index.d.ts",
      "svelte": "./lib/svelte/index.js",
      "default": "./lib/svelte/index.js"
    },
    "./unstable_core": {
      "types": "./lib/core/index.d.ts",
      "require": "./lib/core/index.cjs",
      "default": "./lib/core/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "tsc": "tsc -p . --noEmit",
    "test": "vitest run --project=unit --project=react --project=vue --project=solid",
    "lint": "eslint '{src,e2e}/**/*.{js,jsx,ts,tsx}'",
    "check:svelte": "svelte-check --tsconfig ./tsconfig.svelte.json",
    "storybook": "storybook dev -p 6006",
    "storybook:rtl": "STORYBOOK_RTL=1 npm run storybook",
    "storybook:vue": "STORYBOOK_VUE=1 npm run storybook",
    "storybook:solid": "STORYBOOK_SOLID=1 npm run storybook",
    "storybook:svelte": "STORYBOOK_SVELTE=1 npm run storybook",
    "storybook:build": "storybook build",
    "storybook:deploy": "STORYBOOK_DEPLOY=1 npm run storybook:build && STORYBOOK_VUE=1 npm run storybook:build -- -o storybook-static/vue && STORYBOOK_SOLID=1 npm run storybook:build -- -o storybook-static/solid && STORYBOOK_SVELTE=1 npm run storybook:build -- -o storybook-static/svelte",
    "storybook:test": "vitest run --project=storybook",
    "e2e": "npx playwright test",
    "typedoc": "typedoc",
    "size": "size-limit",
    "prepublishOnly": "npm run typedoc && rimraf lib && npm run build"
  },
  "devDependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "^1.7.0",
    "@atlaskit/pragmatic-drag-and-drop-auto-scroll": "^2.0.0",
    "@atlaskit/pragmatic-drag-and-drop-hitbox": "^1.1.0",
    "@atlaskit/pragmatic-drag-and-drop-react-drop-indicator": "^3.2.0",
    "@babel/plugin-transform-react-pure-annotations": "^7.27.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@faker-js/faker": "^10.0.0",
    "@formkit/auto-animate": "^0.9.0",
    "@mui/material": "^7.3.2",
    "@playwright/test": "^1.50.1",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@solidjs/testing-library": "^0.8.10",
    "@storybook/addon-docs": "^9.1.7",
    "@storybook/addon-vitest": "^9.1.7",
    "@storybook/react-vite": "^9.1.7",
    "@storybook/svelte-vite": "^9.1.7",
    "@storybook/vue3-vite": "^9.1.7",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tanstack/react-virtual": "^3.13.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/react-virtualized": "^9.22.3",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "babel-preset-solid": "^1.9.9",
    "cmdk": "^1.1.1",
    "eslint": "^9.35.0",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc.1",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^27.0.0",
    "re-resizable": "^6.11.2",
    "react": "^19.2.0",
    "react-content-loader": "^7.1.1",
    "react-dom": "^19.2.0",
    "react-is": "^19.2.0",
    "react-merge-refs": "^3.0.2",
    "react-virtualized": "^9.22.6",
    "react-virtuoso": "^4.14.1",
    "react-window": "^2.2.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.1",
    "rollup-plugin-banner2": "^1.3.1",
    "size-limit": "^11.2.0",
    "solid-js": "^1.9.9",
    "storybook": "^9.1.7",
    "storybook-solidjs-vite": "^9.0.3",
    "svelte": "^5.38.9",
    "svelte-check": "^4.3.1",
    "typedoc": "^0.28.4",
    "typedoc-plugin-markdown": "^4.6.3",
    "typedoc-plugin-vue": "^1.5.0",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.43.0",
    "unplugin-vue-jsx": "^0.8.0",
    "virtua": "^0.44.0",
    "vite": "^7.1.9",
    "vite-plugin-solid": "^2.11.9",
    "vitest": "^3.2.4",
    "vue": "^3.4.34",
    "vue-eslint-parser": "^10.2.0"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0",
    "solid-js": ">=1.0",
    "svelte": ">=5.0",
    "vue": ">=3.2"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/virtua.git"
  },
  "keywords": [
    "react",
    "react-component",
    "vue",
    "solid",
    "svelte",
    "ui",
    "headless",
    "list",
    "grid",
    "table",
    "flex",
    "scroll",
    "scroller",
    "scrolling",
    "virtual",
    "virtualized",
    "virtualization",
    "infinite",
    "windowing"
  ],
  "author": "inokawa <stratoooo-taster@yahoo.co.jp> (https://github.com/inokawa/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inokawa/virtua/issues"
  },
  "homepage": "https://github.com/inokawa/virtua#readme"
}
