    "concurrently": "7.2.2",
    "esbuild": "0.15.16",
    "esbuild-loader": "2.20.0",
    "eslint": "8.36.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.0.2",
    "jest-environment-jsdom": "29.0.2",
    "re-resizable": "6.9.9",
    "react": "18.1.0",
